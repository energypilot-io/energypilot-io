{
    "logging": {
        "loggers": ["console", "file"],
        "logLevels": {
            "": "debug"
        },
        "file": {
            "rotatingFiles": true
        }
    },

    "database_": {
        "filename": ":memory:"
    },

    "update": {
        "polling": 5,
        "snapshot": 60
    },

    "connectors": [
        {
            "id": "sungrow_1",
            "type": "modbus",
            "host": "Modbus_IP_as_String",
            "port": 502
        },
        {
            "id": "sungrow_2",
            "type": "modbus",
            "host": "Modbus_IP_as_String",
            "port": 502
        },

        {
            "id": "tapo_1",
            "type": "tapo",
            "email": "your_tapo_email",
            "password": "your_tapo_password",
            "ip": "IP_as_String"
        },

        {
            "id": "abl_emh1",
            "type": "modbus",
            "transport": "ascii",
            "host": "Modbus_IP_as_String",
            "port": 502
        }
    ],

    "devices_": [
        {
            "id": "wallbox1",
            "label": "Wallbox",
            "type": "consumer",
            "template": "abl-emh",
            "connector": "abl_emh1"
        }
    ],

    "devices": [
        {
            "id": "grid_1",
            "type": "grid",
            "template": "sungrow-hybrid",
            "connector": "sungrow_1"
        },

        {
            "id": "pv_1",
            "type": "pv",
            "template": "sungrow-hybrid",
            "connector": "sungrow_1"
        },

        {
            "id": "pv_2",
            "type": "pv",
            "template": "sungrow-hybrid",
            "connector": "sungrow_2"
        },

        {
            "id": "battery_1",
            "type": "battery",
            "template": "sungrow-hybrid",
            "connector": "sungrow_1"
        },

        {
            "id": "antminer1",
            "label": "Antminer",
            "type": "consumer",
            "template": "tapo-p1xx",
            "connector": "tapo_1"
        },

        {
            "id": "wallbox1",
            "label": "Wallbox",
            "type": "consumer",
            "template": "abl-emh",
            "connector": "abl_emh1"
        }
    ]
}
